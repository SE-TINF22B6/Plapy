{
  "name": "plapy",
  "version": "1.0.0",
  "description": "Discord music bot built with discord.js",
  "main": "index.ts",
  "based on": "https://github.com/eritislami/evobot.git",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "dev": "nodemon index.ts",
    "start": "ts-node index.ts",
    "build": "tsc --build",
    "prod": "npm run build && node ./dist/index.js",
    "commit": "cz",
    "format": "npx prettier --write '**/*.ts' --config .prettierrc"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "array-move": "^3.0.1",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^4.4.1",
    "i18n": "^0.15.1",
    "kotlin": "^1.9.20-Beta",
    "lyrics-finder": "^21.0.5",
    "play-dl": "^1.9.7",
    "soundcloud-downloader": "^0.2.3",
    "string-progressbar": "^1.0.4",
    "youtube-sr": "~4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.5.5",
    "@types/jquery": "^3.5.22",
    "@types/node": "^20.8.6",
    "cz-conventional-changelog": "^3.0.1",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "libsodium-wrappers": "^0.7.13",
    "opusscript": "^0.1.0",
    "sodium-native": "^3.4.1",
    "tweetnacl": "^1.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false
    }
  }
}
